@model Recursos_Humanos.Nominas

@{
    ViewBag.Title = "Details";
}

    <section class="single-page-title">
        <div class="container text-center">
            <h2>Nominas</h2>
        </div>
    </section>

    <div class="container body-content topSub">
        <div >
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayName("Año")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Ano)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Mes)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Mes)
                </dd>

                <dt>
                    @Html.DisplayName("Monto Total")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Monto_Total)
                </dd>

            </dl>
        </div>

        <div class="form-group" style="margin-top:20px">           
            <div >
                <div class="btn btn-primary" style="color:black">
                    @Html.ActionLink("Editar", "Edit", new { id = Model.Id })
                </div>
            </div>           
        </div>
        <div class="form-group" >
            <div >
                <div class="btn btn-warning" style="margin-top:20px; margin-bottom:20px; ">
                    @Html.ActionLink("Regresar", "Index")
                </div>
            </div>
        </div>
    </div>
